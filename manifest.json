{
  "manifest_version": 2,
  "name": "isekai",
  "version": "3.3",
  "icons": {
    "128": "icons/isekai.png"
  },
  "permissions": [
    "tabs",
    "storage",
    "cookies",
    "contextualIdentities"
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_icon": "icons/isekai.png"
  }
}
